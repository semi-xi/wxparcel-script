{
  "name": "wxparcel-script",
  "version": "1.0.0-alpha.12.1",
  "license": "MIT",
  "bin": "./bin/cli",
  "main": "./libs/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/wxparcel/wxparcel-script.git"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "docs:esdoc": "rimraf ./gh-pages/esdoc && esdoc -c ./.esdoc.json",
    "docs:used": "cpx './README.md' ./documents && cpx './documents/.nojekyll' ./gh-pages && cpx './documents/**/*' ./gh-pages",
    "docs:used:server": "concurrently \"cpx './README.md' ./documents --watch\" \"docsify serve ./documents\"",
    "docs": "concurrently \"npm run docs:esdoc\" \"npm run docs:used\"",
    "gh-pages": "rimraf gh-pages && npm run docs",
    "yamllint": "yamllint ./*.yml",
    "eslint": "eslint --config .eslintrc.js --ext .js --fix './src/**/*.js'",
    "lint": "npm run yamllint && npm run eslint",
    "mocha": "mocha --require babel-core/register ./test/*.spec.js",
    "coverage": "rimraf ./coverage && nyc --reporter=text --reporter=lcov --report-dir=./coverage npm run mocha",
    "coveralls": "npm run coverage && node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "test": "npm run lint && npm run mocha",
    "develop": "rimraf ./libs && concurrently \"babel ./src --out-dir ./libs --watch\" \"cpx 'src/**/*.handlebars' ./libs --watch --verbose\"",
    "release": "rimraf ./libs && concurrently \"babel ./src --out-dir ./libs\" \"cpx 'src/**/*.handlebars' ./libs --verbose\"",
    "start": "npm run develop"
  },
  "dependencies": {
    "async": "^2.6.1",
    "babel-register": "^6.26.0",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
    "columnify": "^1.5.4",
    "commander": "^2.16.0",
    "finalhandler": "^1.1.1",
    "fs-extra": "^6.0.1",
    "ip": "^1.1.5",
    "ip-port-regex": "^2.0.0",
    "lodash": "^4.17.10",
    "loose-envify": "^1.4.0",
    "minimatch": "^3.0.4",
    "node-sass": "^4.9.2",
    "portscanner": "^2.2.0",
    "pretty-error": "^2.1.1",
    "promise-waterfall": "^0.1.0",
    "serve-static": "^1.13.2",
    "source-map": "^0.7.3",
    "spritesheet-templates": "^10.2.3",
    "spritesmith": "^3.3.1",
    "strip-comment": "^1.1.3",
    "strip-css-comments": "^3.0.0",
    "uglify-es": "^3.3.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "concurrently": "^4.0.1",
    "cpx": "^1.5.0",
    "docsify-cli": "^4.3.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.14.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "rimraf": "^2.6.2",
    "yaml-lint": "^1.2.4"
  }
}
